{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "src/types/*.d.ts"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}